{
  "name": "@evlog/nuxthub",
  "version": "0.0.1-alpha.1",
  "description": "Self-hosted log retention for evlog using NuxtHub database storage",
  "author": "HugoRCD <contact@hrcd.fr>",
  "homepage": "https://evlog.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/evlog.git",
    "directory": "packages/nuxthub"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    },
    "./schema/sqlite": {
      "types": "./dist/schema/sqlite.d.mts",
      "import": "./dist/schema/sqlite.mjs"
    },
    "./schema/postgresql": {
      "types": "./dist/schema/postgresql.d.mts",
      "import": "./dist/schema/postgresql.mjs"
    },
    "./schema/mysql": {
      "types": "./dist/schema/mysql.d.mts",
      "import": "./dist/schema/mysql.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.mts",
  "files": [
    "dist",
    "src/runtime",
    "src/schema",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev:prepare": "tsdown"
  },
  "dependencies": {
    "evlog": "workspace:*",
    "@nuxthub/core": "^0.10.6"
  },
  "devDependencies": {
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "drizzle-orm": ">=0.45.1"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
