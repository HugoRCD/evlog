{
  "name": "@evlog/nuxthub",
  "version": "1.1.0",
  "description": "Self-hosted log retention for evlog using NuxtHub database storage",
  "author": "HugoRCD <contact@hrcd.fr>",
  "homepage": "https://evlog.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HugoRCD/evlog.git",
    "directory": "packages/nuxthub"
  },
  "bugs": {
    "url": "https://github.com/HugoRCD/evlog/issues"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "nuxthub",
    "evlog",
    "logging",
    "nuxt",
    "drizzle",
    "database"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "evlog": "workspace:*",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "evlog": "^1.10.0",
    "@nuxthub/core": "^0.10.6",
    "drizzle-orm": ">=0.45.1"
  },
  "peerDependenciesMeta": {
    "drizzle-orm": {
      "optional": false
    },
    "evlog": {
      "optional": false
    },
    "@nuxthub/core": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
